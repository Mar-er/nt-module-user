{
    "name": "nt-module-user",
    "version": "1.6.2",
    "description": "The user module for Notadd application.",
    "scripts": {
        "start": "ts-node -r tsconfig-paths/register starter/main.ts",
        "start:watch": "nodemon",
        "debug": "node --inspect-brk -r ts-node/register starter/main.ts",
        "debug:watch": "nodemon --config nodemon-debug.json",
        "check": "tslint -p tsconfig.json -c tslint.json",
        "fix": "tslint -p tsconfig.json -c tslint.json --fix",
        "build": "node --max_old_space_size=4096 ./node_modules/gulp/bin/gulp.js build",
        "publish": "./node_modules/.bin/lerna publish -m \"chore(release): publish %s\""
    },
    "author": "notadd",
    "license": "Apache-2.0",
    "private": false,
    "dependencies": {
        "@nestjs/common": "^5.5.0",
        "@nestjs/core": "^5.5.0",
        "@nestjs/graphql": "^5.4.0",
        "@nestjs/typeorm": "^5.2.2",
        "@notadd/addon-sms": "^2.4.2",
        "apollo-server-express": "^2.3.1",
        "bcryptjs": "^2.4.3",
        "graphql": "^14.0.0",
        "graphql-tools": "^4.0.3",
        "graphql-type-json": "^0.2.1",
        "i18n": "^0.8.3",
        "jsonwebtoken": "^8.4.0",
        "moment": "^2.23.0",
        "pg": "^7.7.1",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^6.3.3",
        "typeorm": "^0.2.9",
        "typescript": "^3.2.2"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/graphql": "^14.0.3",
        "@types/graphql-type-json": "^0.1.3",
        "@types/i18n": "^0.8.3",
        "@types/jsonwebtoken": "^8.3.0",
        "@types/node": "^10.12.15",
        "gulp": "^3.9.1",
        "gulp-sequence": "^1.0.0",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-tslint": "^8.1.3",
        "gulp-typescript": "^5.0.0",
        "lerna": "^3.6.0",
        "nodemon": "^1.18.9",
        "ts-node": "^7.0.0",
        "tsconfig-paths": "^3.7.0",
        "tslint": "^5.11.0"
    }
}