{
    "name": "module-user",
    "version": "1.0.0",
    "description": "The user module for Notadd application.",
    "scripts": {
        "start": "ts-node -r tsconfig-paths/register stater/main.ts",
        "start:watch": "nodemon",
        "debug": "node --inspect-brk -r ts-node/register stater/main.ts",
        "debug:watch": "nodemon --config nodemon-debug.json",
        "check": "tslint -p tsconfig.json -c tslint.json",
        "fix": "tslint -p tsconfig.json -c tslint.json --fix",
        "build": "node --max_old_space_size=4096 ./node_modules/gulp/bin/gulp.js build"
    },
    "author": "notadd",
    "license": "Apache-2.0",
    "private": false,
    "dependencies": {
        "@nestjs/common": "^5.3.2",
        "@nestjs/core": "^5.3.3",
        "@nestjs/graphql": "^5.1.2",
        "@nestjs/typeorm": "^5.2.0",
        "apollo-server-express": "^2.0.5",
        "bcryptjs": "^2.4.3",
        "graphql": "^14.0.0",
        "graphql-tools": "^3.0.5",
        "graphql-type-json": "^0.2.1",
        "jsonwebtoken": "^8.3.0",
        "pg": "^7.4.3",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^6.3.1",
        "typeorm": "^0.2.7",
        "typescript": "^3.0.3"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.1",
        "@types/graphql": "^0.13.3",
        "@types/graphql-type-json": "^0.1.2",
        "@types/jsonwebtoken": "^7.2.8",
        "@types/node": "^10.9.4",
        "gulp": "^3.9.1",
        "gulp-rename": "^1.4.0",
        "gulp-sequence": "^1.0.0",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-tslint": "^8.1.3",
        "gulp-typescript": "^5.0.0-alpha.3",
        "lerna": "^3.2.1",
        "nodemon": "^1.18.4",
        "ts-node": "^7.0.0",
        "tsconfig-paths": "^3.4.2",
        "tslint": "^5.11.0"
    }
}
